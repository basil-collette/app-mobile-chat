{
  "expo": {
    "name": "appmobilechat",
    "slug": "appmobilechat",
    "version": "1.0.0",
    "icon": "./assets/img/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/e55c205d-cdcb-4336-95e5-14877ee5243b"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.criss.brierre.appmobilechat"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "jsEngine": "hermes",
    "hermes": true,
    "extra": {
      "eas": {
        "projectId": "e55c205d-cdcb-4336-95e5-14877ee5243b"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
